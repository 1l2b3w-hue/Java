<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itlbw.mapper.ClazzMapper">
    <select id="insert" resultType="com.itlbw.pojo.Clazz">
        insert into clazz(name,room,begin_date,end_date,master_id,subject,create_time,update_time) values (#{name},#{room},#{beginDate},#{endDate},#{masterId},#{subject},#{createTime},#{updateTime})
    </select>

    <select id="getClazzById" resultType="com.itlbw.pojo.Clazz">
        select * from clazz where id = #{id}
    </select>
    <select id="findAll" resultType="com.itlbw.pojo.Clazz">
        select * from clazz
    </select>
</mapper>